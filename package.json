{
  "name": "node12-template-action",
  "version": "0.0.0",
  "private": true,
  "description": "Node 12 template action",
  "main": "lib/main.js",
  "engines": {
    "node": "16"
  },
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  },
  "scripts": {
    "tsc": "tsc --skipLibCheck",
    "build": "parcel build src/main.ts",
    "package": "npm run tsc && npm run package:diff & ncc build --source-map --license licenses.txt",
    "package:diff": "ncc build node_modules/reg-cli/dist/diff.js -o dist && mv ./dist/index.js ./dist/diff.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/node12-template.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "resolutions": {
    "**/@octokit/rest": "16.41.1"
  },
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.0.0",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^1.0.0",
    "@actions/io": "^1.0.0",
    "@actions/tool-cache": "^1.0.0",
    "@types/cpx": "^1.5.0",
    "@types/glob": "^7.1.1",
    "axios": "^0.19.0",
    "cpx": "^1.5.0",
    "glob": "^7.1.4",
    "make-dir": "^3.0.0",
    "node-zip": "^1.1.1",
    "reg-cli": "^0.15.7",
    "semver": "^6.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.3",
    "@types/make-dir": "^2.1.0",
    "@types/node": "^17.0.1",
    "@types/semver": "^7.3.9",
    "@vercel/ncc": "^0.33.1",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "parcel": "^2.0.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4"
  }
}
