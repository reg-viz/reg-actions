{
  "name": "reg-actions",
  "version": "0.0.0",
  "private": true,
  "description": "github actions for reg",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "package:cli": "ncc build node_modules/reg-cli/dist/diff.js -o dist/lib && mv dist/lib/index.js dist/lib/diff.js",
    "package:resources": "cp node_modules/reg-cli/report dist -r && cp node_modules/reg-cli/template dist -r",
    "package": "npm run build && npm run package:cli && npm run package:resources && ncc build --source-map --license licenses.txt -o dist/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bokuweb/reg-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "GitHub",
  "license": "MIT",
  "pnpm": {
    "overrides": {
      "fsevents": "1.2.13.",
      "ansi-regex": "^6.0.1",
      "glob-parent": "^6.0.2",
      "trim-newlines": "^4.0.2",
      "axios": "^1.1.3",
      "kind-of": "^6.0.3",
      "minimist": "^1.2.7",
      "shell-quote": "^1.7.3",
      "braces": "^3.0.2"
    }
  },
  "dependencies": {
    "@actions/artifact": "^2.1.11",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "@types/glob": "^8.1.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "cpy": "^11.1.0",
    "exponential-backoff": "^3.1.1",
    "fast-glob": "^3.3.2",
    "glob": "^10.4.1",
    "loglevel": "^1.9.2",
    "make-dir": "^4.0.0",
    "reg-cli": "^0.18.8"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^22.2.0",
    "@types/adm-zip": "^0.5.5",
    "@types/axios": "^0.14.0",
    "@types/loglevel": "^1.6.3",
    "@types/make-dir": "^2.1.0",
    "@types/node": "^22.7.7",
    "@vercel/ncc": "^0.38.2",
    "typescript": "^5.6.3"
  }
}
